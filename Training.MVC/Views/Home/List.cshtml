
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>List</h2>

<ul>
    @foreach (dynamic p in ViewBag.Albums)
    {
        <li>@p.Title</li>
    }
</ul>

<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>Title</th>
            <th>Last Name</th>
            <th>Username</th>
        </tr>
    </thead>
    <tbody>
    @foreach (dynamic p in ViewBag.Albums)
    {
            <tr>
                <th scope="row">1</th>
                <td>Mark</td>
                <td>Otto</td>
                <td>>@p.Title</td>
            </tr>
    }
    </tbody>
</table>

